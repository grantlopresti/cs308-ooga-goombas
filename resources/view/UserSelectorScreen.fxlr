### User Selector Screen FXLR

<package ooga.view>
<package ooga.view.dynamicUI>
<package javafx.scene.control>
<package javafx.scene.layout>

(VBox alignment=CENTER, layoutX=2%, layoutY=1%, spacing=1%, maxWidth=96%, prefHeight=100%) {
  (user-selector paneWidth=96%, paneHeight=80%)
  (HBox alignment=TOP_CENTER, spacing=1%) {
    (Button text=back, prefWidth=25%, prefHeight=10%, actionTag=back)
    (Button text=select-user, prefWidth=50%, prefHeight=10%, actionTag=begin)
  }
}